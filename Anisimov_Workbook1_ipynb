      "source": [
        "\n",
        "import numpy as np\n",
        "for i in range (10000000):\n",
        "print('–ë—É–¥–µ—Ç–µ –≤–≤–æ–¥–∏—Ç—å –æ–ø–µ—Ä–µ–∞—Ü–∏—é? 1/0')\n",
        "otv = float(input('otv = '))\n",
        "if otv == 1:\n",
        "   s = input(\" –ó–Ω–∞–∫ (+, -, *, /, stepen, sin, cos, expon): \" )\n",
        "   if s not in ('+', '-', '*', '/', 'stepen', 'sin', 'cos', 'expon'):\n",
        "       continue\n",
        "   x = float(input('x = '))\n",
        "   y = float(input('y = '))\n",
        "   if s == '+':\n",
        "print(x + y)\n",
        "elif s == '-':\n",
        "print(x - y)\n",
        "elif s == '*':\n",
        "print(x * y)\n",
        "elif s == '/':\n",
        "       if y! = 0:\n",
        "print(x / y)\n",
        "       else:\n",
        "print(\" –î–µ–ª–µ–Ω–∏–µ–Ω–∞–Ω–æ–ª—å! \" )\n",
        "elif s == 'expon':\n",
        "       print(np. exp(x+y))\n",
        "elif s == 'stepen':\n",
        "       print(x**y)\n",
        "elif s == 'sin':\n",
        "       print(np. sin(x+y))\n",
        "elif s == 'cos':\n",
        "       print(np. cos(x+y))\n",
        "if otv == 0:\n",
        "  break\n",
        "if otv not in (0, 1):\n",
        "      continue"
